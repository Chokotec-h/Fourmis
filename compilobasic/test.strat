

while (true) do {

    if (= leftahead friend) then
    {
        move;
        turn left
    }
    else
    {
        ifs (= rightahead friend) then
        {
            move;
            turn right
        }
    };



    ifs (= here food) then
    {
        ifs(!= here home) then
        {
            pickup
        }
    };
    
    ifs (= here home) then
    {
        drop
    };
    ifs (random 20) then
    {
        turn left
    };
    if (= ahead rock) then {
        turn left
    }
    else
    {
        if (= ahead underground) then {
            dig;
            move
        }
        else{
            move
        }
    }
}